{
  "name": "exi-paper",
  "version": "0.0.1",
  "main": "index.js",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "MDwalks-UI": "https://github.com/linewalks/MDwalks-UI#v0.13.37",
    "antd": "^4.4.3",
    "axios": "^0.19.0",
    "axios-mock-adapter": "^1.17.0",
    "babel-plugin-import": "^1.12.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-adapter-react-16.3": "^1.7.2",
    "identity-obj-proxy": "^3.0.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.15",
    "next": "^9.3.0",
    "next-cookies": "^2.0.3",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.1",
    "react": "^16.12.0",
    "react-content-loader": "^4.2.2",
    "react-dom": "^16.12.0",
    "react-hook-form": "^3.18.4",
    "sass": "^1.26.9",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start -p $PORT",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint ./components/**/*.js",
    "lint:fix": "eslint ./components/**/*.js --fix",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn test"
    }
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "./setupTest.js"
    ],
    "moduleNameMapper": {
      "^@components(.*)$": "<rootDir>/components/$1",
      "^@public(.*)$": "<rootDir>/public/$1",
      "^@Table(.*)$": "<rootDir>/src/components/table/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass)$": "identity-obj-proxy"
    },
    "watchPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/build/"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.6",
    "conventional-changelog-cli": "^2.0.34",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^4.2.3",
    "jest": "24.7.1",
    "mutationobserver-shim": "^0.3.3"
  }
}
